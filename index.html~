<?php include 'email.php';?>
<script>

	$("#send_message").click(function () {

		var data = {
			name: $("#name").val(),
			email: $("#email").val(),
			message: $("#message").val()
		};
		$.ajax({
			type: "POST",
			url: "email.php",
			data: data,
			success: function(){
				$('.success').fadeIn(1000);
			}
		});

		console.log(data.name);
		console.log($.ajax.url);
	});

</script>
